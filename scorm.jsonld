      }
    },
    {
      "type": "Verb",
      "id": "http://adlnet.gov/expapi/verbs/interacted",
      "inScheme": "https://w3id.org/xapi/scorm",
      "prefLabel": {
        "en": "interacted"
      },
      "definition": {
